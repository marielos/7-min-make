
<div id="part2">
  <%if @photo.errors.any?%>
    <div id="error_explanation">
      <h2><%=pluralize(@photo.errors.count, "error")%>
          prohibited this photo from being saved:</h2>
      <ul>
        <%@photo.errors.full_messages.each do |msg|%>
          <li><%=msg%></li>
        <%end%>
      </ul>
    </div>
  <%end%>

  <%=form_for @photo,:url => "/maker.herokuapp.com/prompts/1/photos/1",:html=>{:multipart=>true} do |form|%> 
    <table id="submit">
    	<tr>
        <td><%=form.label ("Name:")%></td>
        <td><%=form.text_field (:user_name)%></td>
    	</tr>
    	
    </table>
  <%end%>
</div>


<script type = "text/javascript"> 
$("#photo_photo").change(function(){
    readURL(this);
});

$("#submitForm").click(function(){
});

</script> 
