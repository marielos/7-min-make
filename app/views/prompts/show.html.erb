<h2><%=@prompt.title%></h2>

<div class = "button">
<%=link_to "Submit creation and REDIRECT", new_prompt_photo_path(:id)%>
</div>



<%for photo in @prompt.photos do%>
	<div class = "entry">
		<%=photo.file_name%>
		<br>
		<%=image_tag photo.file_name%>
		<br>
		<%=photo.user_name%>
	</div>
<%end%>

